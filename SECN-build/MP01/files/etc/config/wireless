
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option 'disabled' '0'
	option 'channel' '1'
	option 'diversity' '0'
	option 'txpower' '17'
	option 'chanbw' '20'
	option 'hwmode' '11g'

config 'wifi-iface' 'ap_0'
	option 'device' 'radio0'
	option ifname 'wlan0'
#	option 'network' 'lan'
	option 'mode' 'ap'
	option 'key' 'potato-potato'
	option 'ssid' 'VT-SECN-AP'
	option 'encryption' 'psk2' 
	option 'maxassoc' '20'
	option hidden '0'
	option 'disabled' '0'

config 'wifi-iface' 'ah_0'
	option 'device' 'radio0'
	option ifname 'wlan0-1'
	option 'encryption' 'none'
	option 'mode' 'adhoc'
	option 'bssid' '02:CA:FF:EE:BA:BE'
#	option 'network' 'mesh_0'
	option 'hidden' '0'
	option 'ssid' 'vt-mesh'
	option 'disabled' '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
#	option network 'wan'
	option ssid 'host-ssid'
	option key 'host-passphrase'
	option encryption 'psk'
	option disabled '1'

