
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy1'
	option hwmode '11g'
	option htmode 'HT20'
	option channel '1'
	option txpower '12'
	option chanbw '20'
	option disabled '0'
	option country ''
	option coverage '5'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option maxassoc '30'
	option ssid 'VT-SECN-AP'
	option encryption 'psk2'
	option key 'potato-potato'
	option hidden '0'
	option disabled '0'
	option isolate '0'

config wifi-iface 'ah_0'
	option device 'radio0'
	option ifname 'wlan0-1'
	option mode 'mesh'
	option network 'mesh_0'
	option mesh_id 'vtmesh'
	option encryption 'none'
	option disabled '1'
	option hidden '0'
	option mesh_fwding '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option key 'host-passphrase'
	option encryption 'psk'
	option disabled '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '157'
	option phy 'phy0'
	option hwmode '11a'
	option htmode 'VHT40'
	option chanbw '20'
	option disabled '0'
	option country ''
	option coverage '5'
	option doth '0'
	option txpower '20'
	option band '5G'

config wifi-iface 'ap_1'
	option device 'radio1'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option ssid 'VT-SECN-AP-1'
	option encryption 'psk2'
	option key 'potato-potato'
	option hidden '0'
	option maxassoc '30'
	option disabled '1'
	option isolate '0'

config wifi-iface 'ah_1'
	option device 'radio1'
	option ifname 'wlan1-1'
	option network 'mesh_1'
	option mode 'mesh'
	option mesh_id 'vtmesh'
	option disabled '0'
	option hidden '0'
	option encryption 'none'
	option mesh_fwding '0'

config wifi-iface 'sta_1'
	option device 'radio1'
	option ifname 'wlan1-2'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option key 'host-passphrase'
	option encryption 'psk'
	option disabled '1'

