config wifi-device 'radio0'
	option type    'mac80211'
	option path    'pci0000:00/0000:00:00.0'
	option htmode  'HT20'
	option channel '1'
	option txpower '17'
	option chanbw  '20'
	option hwmode  '11ng'
	option disabled '0'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option maxassoc '20'
	option ssid 'VT-SECN-AP'
	option encryption 'psk2'
	option key 'potato-potato'
	option hidden '0'
	option disabled '0'
	option isolate '0'

config wifi-iface 'ah_0'
	option device 'radio0'
	option ifname 'wlan0-1'
	option network 'mesh_0'
	option encryption 'none'
	option bssid '02:CA:FF:EE:BA:BE'
	option ssid 'vt-mesh'
	option mode 'adhoc'
	option hidden '0'
	option disabled '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option encryption 'psk'
	option disabled '1'
	option key 'host-password'


