
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT20'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option regdomain '0x35'
	option channel '1'
	option txpower '17'
	option chanbw '20'
	option hwmode '11g'
	option country '276'
	option disabled '0'

config wifi-iface 'ah_0'
	option device 'radio0'
	option network 'mesh_0'
	option encryption 'none'
	option bssid '02:CA:FF:EE:BA:BE'
	option ssid 'vt-mesh'
	option disabled '0'
	option mode 'adhoc'
	option hidden '0'

config wifi-iface 'ap_0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option maxassoc '20'
	option ssid 'VT-SECN-AP'
	option encryption 'psk'
	option key 'potato-potato'
	option hidden '0'
	option disabled '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option key 'host-passphrase'
	option encryption 'psk'
	option disabled '1'

