config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option channel '1'
	option hwmode '11g'
	option 'txpower' '17'
	option 'diversity' '0'
	option disabled '0'
	option country '276'
	option regdomain '0x35'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option maxassoc '20'
	option encryption 'psk'
	option key 'potato-potato'
	option hidden '0'
	option disabled '0'
	option ssid 'VT-SECN-AP'

config wifi-iface 'ah_0'
	option device 'radio0'
	option ifname 'wlan0-1'
	option network 'mesh_0'
	option encryption 'none'
	option bssid '02:CA:FF:EE:BA:BE'
	option ssid 'vt-mesh'
	option disabled '0'
	option mode 'adhoc'
	option hidden '0'

config wifi-iface 'sta_0'
	option device 'radio0'
	option mode 'sta'
	option network 'wan'
	option ssid 'host-ssid'
	option key 'host-passphrase'
	option encryption 'psk'
	option disabled '1'

