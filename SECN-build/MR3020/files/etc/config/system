config system
	option timezone 'UTC'
	option hostname 'TP-20'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enable_server '0'

config button
        option button 'wps'
        option action 'released'
        option handler '/bin/factory-restore.sh && reboot'
        option min '10'
        option max '20'

config button
        option button 'wps'
        option action 'released'
        option handler '/bin/firmware-restore.sh && reboot'
        option min '20'
        option max '30'

config led 'led_usb'
        option name 'USB'
        option sysfs 'tp-link:green:3g'
        option trigger 'usbdev'
        option dev '1-1'
        option interval '50'

config led 'led_wlan'
        option name 'WLAN'
        option sysfs 'tp-link:green:wlan'
        option trigger 'phy0tpt'

config led 'led_lan'
        option name 'LAN'
        option sysfs 'tp-link:green:lan'
        option trigger 'netdev'
        option dev 'eth0'
        option mode 'link tx rx'

