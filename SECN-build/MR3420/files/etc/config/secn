
config mesh 'asterisk'
	option codec1 'gsm'
	option codec2 'ulaw'
	option codec3 'alaw'
	option host 'sip.myhost.com'
	option fromdomain 'sip.myhost.com'
	option dialout '#'
	option externip '0.0.0.0'
	option reghost 'sip.myhost.com'
	option softph 'OFF'
	option username 'myuser'
	option fromusername 'myuser'
	option enable '0'
	option register '0'
	option enablenat '0'
	option enable_ast '0'

config mesh 'accesspoint'
	option ssid 'VT-SECN-AP'
	option encryption 'WPA1'
	option passphrase 'potato-potato'
	option ap_disable '0'
	option usreg_domain '0'
	option maxassoc '20'

config mesh 'dhcp'
	option enable '0'
	option startip '10.130.1.200'
	option endip '10.130.1.240'
	option leaseterm '7200'
	option domain 'lan'
	option subnet '255.255.255.0'
	option router '10.130.1.1'
	option maxleases '40'
	option dhcp_auth '0'

config mesh 'mpgw'
	option mode 'OFF'

config mesh 'http'
	option limitip '0'
	option enssl '0'
	option pw_preset '0'
	option auth '0'

config mesh 'ivr'
	option pin '1234'

config mesh 'modem'
	option enabled 0
	option vendor ''
	option product ''
	option modemport '0'
	option dialstr '*99#'
	option service 'umts'
	option pin ''
	option apn ''
	option username ''
	option password ''

config mesh wan
	option wanip '10.0.0.100'
	option wangateway '10.0.0.1'
	option wanmask '255.255.255.0'
	option wandns '8.8.8.8'
	option wanssid 'host-ssid'
	option wanpass 'host-password'
	option wanencr 'psk'
	option wanport 'Disable'
	option ethwanmode 'DHCP'


