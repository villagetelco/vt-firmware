
config secn 'asterisk'
	option codec1 'gsm'
	option codec2 'ulaw'
	option codec3 'alaw'
	option host 'sip.myhost.com'
	option fromdomain 'sip.myhost.com'
	option dialout '#'
	option externip '0.0.0.0'
	option reghost 'sip.myhost.com'
	option softph 'OFF'
	option username 'myuser'
	option fromusername 'myuser'
	option enable '0'
	option register '0'
	option enablenat '0'
	option enable_ast '0'

config secn 'radio'
	option duomode 'Pri_AP..Sec_Mesh'

config secn 'accesspoint'
	option ssid 'VT-SECN-AP'
	option passphrase 'potato-potato'
	option ap_disable '0'
	option maxassoc '20'
	option ap_isol '0'
	option encryption 'WPA2'

config secn 'mesh'
	option mpgw 'OFF'
	option mesh_disable '0'

config secn 'dhcp'
	option enable '0'
	option startip '10.130.1.200'
	option endip '10.130.1.240'
	option leaseterm '7200'
	option domain 'lan'
	option subnet '255.255.255.0'
	option router '10.130.1.254'
	option maxleases '40'
	option dhcp_auth '0'
	option dns '8.8.8.8'
	option dns2 '8.8.4.4'
	option device_ip 'checked'

config secn 'http'
	option limitip '0'
	option enssl '0'
	option pw_preset '0'
	option auth '0'

config secn 'ivr'
	option pin '1234'

config secn 'modem'
	option modemport '0'
	option service 'umts'
	option dialstr '*99#'
	option url     '/nomodem.html'
	option apn 'my.apn'
	option vendor '1234'
	option product '5678'

config secn 'wan'
